        bool math=false,
        bool prob=false,
        bool listbox=false,
        bool defbox=false,
        bool code=false,
        bool fancychap=false,
        bool pkgusage=false,
        bool playcontent
]
{%
        \foreach \@test@context in \@test@contexts%
        {%
                \ifnumequal{\xp\commandkey\xp{\@test@context}}{1}%
                        {\gxp\csuse\xp{\@test@context testtrue}}%
                        {\gxp\csuse\xp{\@test@context testfalse}}%
        }%
        \ifOr{\ifmacrotest/if, \ifmisctest/if}{\input{C:/Users/Blake/Documents/LaTeX/Documentation/cs-examples.tex}}{}%
}
