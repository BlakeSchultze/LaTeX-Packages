\DeclareDocumentCommand{\setPaper@orientation}{s m}
{%
        \ifOr{\ifmylatex@landscape/if,#1/xparse}%
        {%      set landscape orientation\\
                \setPaper@dim{current@paperheight}[#2]{@paperwidth}%
                \setPaper@dim{current@paperwidth}[#2]{@paperheight}%
                \setPaper@dim{current@textheight}[#2]{@textwidth}%
                \setPaper@dim{current@textwidth}[#2]{@textheight}%%
                \xdef\geometry@args{landscape}%
                \setlength\paperheight{\current@paperheight}%
                \setlength\paperwidth{\current@paperwidth}%
                \setlength\textheight{\current@textheight}%
                \setlength\textwidth{\paperwidth-\current@left-\current@right}%
                \setlength\linewidth{\paperwidth-\current@left-\current@right}%
                \setlength\headwidth{\the\textwidth}%
                \setlength\@landscape@page@yshift{\dimexpr\current@paperwidth - \current@paperheight\relax}%+15pt
                \setlength\@landscape@margin@yshift{\dimexpr\current@right - \current@top\relax}%
                \setlength\@landscape@line@shift{\dimexpr\@tikzheading@linewidth/2\relax}%
