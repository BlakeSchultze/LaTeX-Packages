\DeclareDocumentCommand{\drawxaxis}{G{5cm} G{8} G{0.25} O{\small}}
{%
	%\setlength{\unitlength}{4mm}%
	%\setlength{\@temp@length}{4mm}%
	\dimdef\@xaxis@length{#1}%
	\numdef\@num@ticks{#2}%
        \numdef\@tick@increment{#3}%
        \def\@xaxis@font@cmds{#4}%
        %\stripunits\@xaxis@length[\@xaxis@length@numpts]%
        \pgfmathsetmacro\@xaxis@length@numpts{\@xaxis@length}%
	\pgfmathsetmacro\@tick@start@pos@numpts{\@tick@start@pos}%
	\pgfmathsetmacro\@tick@length@numpts{\@tick@length}%
	\pgfmathsetmacro\@xaxis@incremen@numpts{\@xaxis@length@numpts / \@num@ticks}%
	\@xaxis@font@cmds%
	\begin{picture}(\@num@ticks,1)(0,-.5)%
	        \put(0,0){\vector(1,0){\@xaxis@length@numpts}}%
	        \multido{\nD=0+\@xaxis@incremen@numpts,\nX=0+\@tick@increment}{\@num@ticks}{%
	        \put(\nD,-\@tick@start@pos@numpts){\line(0,1){\@tick@length@numpts}}%
	        \put(\nD,-\@tick@length@numpts){\makebox(0,0)[t]{\nX}}}%
	\end{picture}\\\\
}
