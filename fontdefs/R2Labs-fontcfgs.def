\WithSuffix\newcommand\large*{\fontsize{12}{\baselinestretch\baselineskip}}
\WithSuffix\newcommand\midLarge*{\fontsize{13}{\baselinestretch\baselineskip}}
\WithSuffix\newcommand\Large*{\fontsize{14}{\baselinestretch\baselineskip}}
\WithSuffix\newcommand\midLARGE*{\fontsize{15}{\baselinestretch\baselineskip}}
\WithSuffix\newcommand\LARGE*{\fontsize{16}{\baselinestretch\baselineskip}}
\WithSuffix\newcommand\midhuge*{\fontsize{17}{\baselinestretch\baselineskip}}
\WithSuffix\newcommand\huge*{\fontsize{18}{\baselinestretch\baselineskip}}
\WithSuffix\newcommand\midHuge*{\fontsize{19}{\baselinestretch\baselineskip}}
