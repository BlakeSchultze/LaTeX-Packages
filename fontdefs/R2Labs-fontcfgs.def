%~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~%
%~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ Text style/property configuration commands ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~%
%~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~%
%-------------------------------------------------------------------------------------------------------------------------------%
\newcommand\@unslant[1]%
{%
        \def\@slant@val{-0.25}%
        \ifdefequal{#1}{\delta}{\def\@slant@val{-0.15}}{}%
        \ifdefequal{#1}{\epsilon}{\def\@slant@val{-0.1}}{}%
        \ifdefequal{#1}{\xi}{\def\@slant@val{-0.15}}{}%
        \ifdefequal{#1}{\zeta}{\def\@slant@val{-0.15}}{}%
        \slantbox[\@slant@val]{\ensuremath{#1}}%
        %\unslant[\@slant@val]{#1}%
        %\slantbox[\@slant@val]{#1}
        %\slantbox[\@slant@val]{$\csname #1\endcsname$}
}
%-------------------------------------------------------------------------------------------------------------------------------%
\newcommand\unslant[1]{\trim@spaces{\@unslant{#1}}}%
%-------------------------------------------------------------------------------------------------------------------------------%
\DeclareDocumentCommand{\RaisedText}{m}
{%
        \begingroup
                \leavevmode
                \rlap{\kern-.4pt\raise.2pt\hbox{\fontcolor{white}#1}}%
                \rlap{\kern.4pt\raise-.2pt\hbox{\fontcolor{DarkSlateGrey!70!}#1}}%
                \hbox{#1}%
        \endgroup
}
%-------------------------------------------------------------------------------------------------------------------------------%
\DeclareDocumentCommand{\BeveledText}{m}
{%
        \begingroup
                \leavevmode
                \rlap%
                {%
                        \kern-.3pt\raise.15pt%
                        \hbox{\pdfrender{TextRenderingMode=Stroke, LineWidth=.2pt, StrokeColor=white}#1}%
                }%
                \rlap%
                {%
                        \kern.3pt\raise-.15pt%
                        \hbox{\pdfrender{TextRenderingMode=Stroke, LineWidth=.2pt, StrokeColor=black}#1}
                }%
                \rlap{\pdfrender{TextRenderingMode=Stroke, LineWidth=.2pt, StrokeColor=DarkSlateGrey}#1}%
                \hbox{#1}%
        \endgroup
}
%-------------------------------------------------------------------------------------------------------------------------------%
% Delay expansion of text cmds, allowing passing in args of commands and titles, e.g. section/figure/header/etc. titles
%-------------------------------------------------------------------------------------------------------------------------------%
\DeclareDocumentCommand{\protexterm}{s m}{\noexpand\noexpand#1}
\DeclareDocumentCommand{\protexted}{s m m}
{%
        \IfBooleanTF{#1}%
                {\xp\noexpand\xp\noexpand\csname #2\endcsname{#3}}%
                {\xp\noexpand\xp\noexpand\csname text#2\endcsname{#3}}%
}
