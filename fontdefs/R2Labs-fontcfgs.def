\DeclareDocumentCommand{\lookup@text@encoding}{m}%
{%type=text,ordinal,roman,Roman
        \xdef\@@text@encoding{}%
        \foreach \@@encoding@raw/\@@encoding@text in \inuse@text@encoding@pairs%
        {%
                \IffStrEq{\@@encoding@raw}{#1}%
                {%
                        \xdef\@@text@encoding{\@@encoding@text}%
                        \breakforeach%
                }%
        }%
        \iffdefempty{\@@text@encoding}{\@latex@error{Unknown font encoding #1}}%
}
%-------------------------------------------------------------------------------------------------------------------------------%
% Strip '-' characters from font family name and define macros containing the names of macros defined/accessed elsewhere
\DeclareDocumentCommand{\lookup@family@text@names}{m}%
{%{(raw)font}
        \xdef\@family@text@name{}%
        \xp\@tfor\xp\nextfamchar\xp:\xp=#1\do%
        {%
                \IfStrEq{\nextfamchar}{-}{}%
                        {\xdef\@family@text@name{\@family@text@name\nextfamchar}}%
        }%
        \xdef\@family@text@prefix{\@family@text@name family}%
        \xdef\@family@encoding@text@names{\@family@text@name family@encodings}%
        \xdef\@family@encoding@text@name{\@family@text@name family@encoding}%
        \xdef\@family@text@encoding@name{\@family@text@name family@text@encoding}%
        \xdef\@family@text@encoding@cmdname{\@family@text@name family@\csuse{\@family@text@encoding@name}}%
        %\csuse{\@family@text@encoding@name}
        %\xdef\@@the@encoding{\csuse{\@family@text@encoding@name}}%
        %\csuse{\@family@text@name family@\@@the@encoding}%
}%
%-------------------------------------------------------------------------------------------------------------------------------%
\DeclareDocumentCommand{\setfamilyencoding}{m m}%
{%{(raw)family}{(raw)encoding}
        \lookup@family@text@names{#1}%
        \letcs\@@match@encodings{\@family@encoding@text@names}%
        \ifcsvmatch{#2}{\@@match@encodings}%
                {\csxdef{\@family@encoding@text@name}{#2}}%
                {\@latex@error{Encoding #2 undefined for #1 font family}}%
        \lookup@text@encoding{\csuse{\@family@encoding@text@name}}%
        \csxdef{\@family@text@encoding@name}{\@@text@encoding}%
}
%-------------------------------------------------------------------------------------------------------------------------------%
\DeclareDocumentCommand{\@init@family@text@encodings}{o}%
{%
        \foreach \@@@family [count=\@@count] in \@inuse@font@families%
        {%
                \lookup@family@text@names{\@@@family}%
                \setfamilyencoding{\@@@family}{\csuse{\@family@encoding@text@name}}%
        }%
}
%-------------------------------------------------------------------------------------------------------------------------------%
\DeclareDocumentCommand{\def@inuse@text@encodings}{O{text}}%
{%type=text(default),ordinal,roman,Roman
        \numtextwords[type=#1,print=false,csvpair=false]{\@inuse@font@encodings}%
        \xdef\inuse@text@encodings{\@@thecsvlist}%
}
%-------------------------------------------------------------------------------------------------------------------------------%
\DeclareDocumentCommand{\def@inuse@text@encoding@pairs}{O{text}}%
{%type=text(default),ordinal,roman,Roman
        \numtextwords[type=#1,print=false,csvpair=true]{\@inuse@font@encodings}%
        \xdef\inuse@text@encoding@pairs{\@@thecsvlist}%
}
%-------------------------------------------------------------------------------------------------------------------------------%
\DeclareDocumentCommand{\def@family@encoding@cmd}{m m m}%
{%{family+encoding macro name}{(raw)encoding}{(raw)family}
        \iffcsundef{#1}%
        {%
                \def@csvadd*{\csxdef}{#1}%
                        {\noexpand\changefont{#2}{#3}}%
                        {\@font@familyenc@macros}%
        }%
}
%-------------------------------------------------------------------------------------------------------------------------------%
\DeclareDocumentCommand{\def@family@encoding@cmds}{O{text} m}%
{%[numToText type=text(default),ordinal,roman,Roman]{(raw)family}
        \lookup@family@text@names{#2}%
        \csxdef{\@family@text@name family@enc@numtext@type}{text}%
        \numtextwords[type=#1,print=false,csvpair=true]{\csuse{\@family@encoding@text@names}}%
        \foreach \@@encoding@raw/\@@encoding@text [count=\@@count] in \@@thecsvlist%
                {\def@family@encoding@cmd{\@family@text@name family@\@@encoding@text}{\@@encoding@raw}{#2}}
}
%-------------------------------------------------------------------------------------------------------------------------------%
\DeclareDocumentCommand{\@beginfontfamily}{o m}%
{%[(raw)encoding]{(raw)family}
        \lookup@family@text@names{#2}%
        \IfValueT{#1}{\setfamilyencoding{#2}{#1}}%
        \xdef\@@the@encoding{\csuse{\@family@text@encoding@name}}%
        \csuse{\@family@text@name family@\@@the@encoding}%
}
%-------------------------------------------------------------------------------------------------------------------------------%
\DeclareDocumentCommand{\def@fontfamily@cmd}{m O{init}}%
{%{(raw)family}[NA]
        \lookup@family@text@names{#1}%
        \csname@nY\NewDocumentCommand{#1family}{o}%\csname@nY\NewDocumentCommand
                {\forwardValueTF\@beginfontfamily{##1}{#1}}%
}
%-------------------------------------------------------------------------------------------------------------------------------%
\DeclareDocumentCommand{\def@fontfamily@cmds}{o}%
{%
        \resettoks{cmdtoks}%
        \foreach \@@@family [count=\@@count] in \@inuse@font@families%
        {%
                \xp\lookup@family@text@names\xp{\@@@family}%
                \xp\def@family@encoding@cmds\xp{\@@@family}%
                \xp\xp\xp\apptotoks\xp\xp\xp{\xp\def@fontfamily@cmd\xp{\@family@text@name}[#1]}{cmdtoks}%
                %\@unique@add@to@csvlist{\@family@text@name}{\@font@familyenc@cmds}%
                \xp\@add@to@csvlist\xp{\@family@text@prefix}{\@font@familyenc@cmds}%
        }%
        \printtoks{cmdtoks}%
}
