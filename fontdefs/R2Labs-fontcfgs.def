%-------------------------------------------------------------------------------------------------------------------------------%
%~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~%
%~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ Font color command definitions ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~%
%~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~%
%-------------------------------------------------------------------------------------------------------------------------------%
%-------------------------------------------------------------------------------------------------------------------------------%
\def\set@cf@color{\color{\cf@color}}%
%-------------------------------------------------------------------------------------------------------------------------------%
\DeclareDocumentCommand{\fontcolor}{s t! m g}
{%
        \IfBooleanTF{#1}{}{\def\cf@color{#3}}%
        \IfBooleanTF{#2}{}{\color{\cf@color}}%
        \IfValueTF{#4}{\textcolor{\cf@color}{#4}}{}%
}
%-------------------------------------------------------------------------------------------------------------------------------%
\DeclareDocumentCommand{\coloredtext}{s t! m m}
{%
        %\begingroup%
        \IfBooleanTF{#1}
        {%
                \IfBooleanTF{#2}
                        {\ignorespaces\textcolor{#3}{\textbf{\textit{#4}}}\unskipspacing}
                        {\ignorespaces\textcolor{#3}{\textbf{#4}}\unskipspacing}
        }
        {%
                \IfBooleanTF{#2}
                        {\ignorespaces\textcolor{#3}{\textit{#4}}\unskipspacing}
                        {\ignorespaces\textcolor{#3}{#4}\unskipspacing}
        }
        %\endgroup%
}
%-------------------------------------------------------------------------------------------------------------------------------%
%~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~%
%~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ Shadow text/math commands ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~%
%~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~%
%-------------------------------------------------------------------------------------------------------------------------------%
\long\def\shadowfypars#1\par#2\relax%
{%
        \ifstrempty{#1} {}{\shadowfywords#1 \relax\relax}%
        \ifstrempty{#2}{}{\par\shadowfypars#2\relax}%
}
%-------------------------------------------------------------------------------------------------------------------------------%
\long\def\shadowfywords#1 #2\relax%
{%
        \ifstrempty{#1}{}{\shadowfyletters#1\relax\relax}%
        \ifstrempty{#2}{}{\ \shadowfywords#2\relax}%
        \unskip%
}
%-------------------------------------------------------------------------------------------------------------------------------%
\long\def\shadowfyletters#1#2\relax%
{%
        \shadow{#1}%
        \ifx\relax#2\else\shadowfyletters#2\relax\fi\unskip%
}
%-------------------------------------------------------------------------------------------------------------------------------%
\long\def\shadowmathfypars#1\par#2\relax%
{%
        \unskipspacing\ifstrempty{#1}{\unskipspacing\noindent}{\unskipspacing\noindent\shadowmathfymathparse#1 $$\relax\noindent}%
        \ifstrempty{#2}{\unskipspacing\noindent}{\unskipspacing\noindent\shadowmathfypars#2\relax\noindent}%
}
%-------------------------------------------------------------------------------------------------------------------------------%
\long\def\shadowmathfylines#1\\#2\relax%
{%
        \ifstrempty{#1}{}{\unskipspacing\noindent\shadowmathfymathparse#1 $$\relax}%
        \ifstrempty{#2}{}{\unskipspacing\noindent\\\shadowmathfylines#2\relax\unskipspacing}%
}
%-------------------------------------------------------------------------------------------------------------------------------%
\long\def\shadowmathfymathparse#1 $#2$#3\relax%
{%
        \ifstrempty{#1}{}{\shadowfypars#1\par\relax\relax\noindent}%
        \ifstrempty{#2}{}{\noindent\space\mathshadow{#2}\space}%
        \ifstrempty{#3}{}{\noindent\shadowmathfywords#3\relax}%
}
%-------------------------------------------------------------------------------------------------------------------------------%
\long\def\shadowmathfywords#1 #2\relax%
{%
        \ifstrempty{#1}{}{\shadowmathfyletters#1\relax\relax}%
        \ifstrempty{#2}{}{\shadowmathfymathparse #2\relax}%
}
%-------------------------------------------------------------------------------------------------------------------------------%
\long\def\shadowmathfyletters#1#2\relax%
{%
        \shadow{#1}%
        \ifx\relax#2\else\shadowmathfyletters#2\relax\fi%
}
%-------------------------------------------------------------------------------------------------------------------------------%
\long\def\doubleshadowfypars#1\par#2\relax%
{%
        \ifstrempty{#1}{}{\noindent\doubleshadowfywords#1 \relax\relax}%
        \ifstrempty{#2}{}{\unskipspacing\par\noindent\doubleshadowfypars#2\relax}%
}
%-------------------------------------------------------------------------------------------------------------------------------%
\long\def\doubleshadowfywords#1 #2\relax%
{%
        \ifstrempty{#1}{}{\doubleshadowfyletters#1\relax\relax}%
        \ifstrempty{#2}{}{\ \doubleshadowfywords#2\relax}%
        \unskip\ignorespaces%
}
%-------------------------------------------------------------------------------------------------------------------------------%
\long\def\doubleshadowfyletters#1#2\relax%
{%
        \kern\st@shadowoffsetx\doubleshadow{#1}%
        \ifx\relax#2\else\doubleshadowfyletters#2\relax\fi\unskip%
}
%-------------------------------------------------------------------------------------------------------------------------------%
\long\def\shadow#1%
{%
        \kern\st@shadowoffsetx%
        \raisebox{\st@shadowoffsety}%
        {%
                \stackinset{c}{-\st@shadowoffsetx}{c}{-\st@shadowoffsety}%
                        {\fontcolor{\st@textcolor}{#1}}%
                        {\fontcolor{\st@shadowcolor}{#1}}%
        }%
        \kern-\st@shadowoffsetx%
}
%-------------------------------------------------------------------------------------------------------------------------------%
\long\def\mathshadow#1{\shadow{\ensuremath{#1}}}
%-------------------------------------------------------------------------------------------------------------------------------%
\long\def\doubleshadow#1%
{%
        \kern\st@doubleshadowoffsetx%
        \raisebox{\st@doubleshadowoffsety}%
        {%
                \stackinset{c}{\st@doubleshadowoffsetx}{c}{-\st@doubleshadowoffsety}%
                        {\shadow{#1}}%
                        {\fontcolor{\st@doubleshadowcolor}{#1}}%
        }%
        \kern-\st@doubleshadowoffsetx%
}
%-------------------------------------------------------------------------------------------------------------------------------%
\newcommand\shadowfy[1]{\xp\shadowfypars#1\par\relax\relax}
\newcommand\shadowmathfy[1]{\unskipspacing\xp\shadowmathfypars#1\par\relax\relax\relax\noindent}
\newcommand\doubleshadowfy[1]{\xp\doubleshadowfypars#1\par\relax\relax}
%-------------------------------------------------------------------------------------------------------------------------------%
%                                            1 2          3         4        5          6
\DeclareDocumentCommand{\setshadowTextProps}{s D<>{0.5pt} O{0.75pt} O{0.2pt} D''{white} D(){black}}
{%\setshadowTextProps<offset>[offset x][offset y]{shadow color}
        \shadowoffset{#2}%
        \shadowoffsetx{#3}%
        \shadowoffsety{#4}%
        \shadowbasecolor{#5}%
        \shadowcolor{#6}%
}
%-------------------------------------------------------------------------------------------------------------------------------%
%                                    1 2                  3            4         5            6 7   8
\DeclareDocumentCommand{\shadowText}{s D::{\anttmcap16pt} D<>{0.75pt} D<>{0.2pt} D''{current} o d() m}
{%\shadowText|title font|'color style'{title text}
        \ignorespaces\unskip\setshadowTextProps[#3][#4]'#5-coltext'(#5-coltextshadow)%
        \IfBooleanTF{#1}
                {\ignorespaces\unskip\shadowbasecolor{#5-coltitle}}%
                {\ignorespaces\unskip\shadowbasecolor{#5-coltext}}%
        \IfValueTF{#6}%
                {\ignorespaces\unskip\shadowcolor{#6}}%
                {}%
        \IfValueTF{#7}%
                {\ignorespaces\unskip#2\shadowbasecolor{#7}}%
                {}%
        \ignorespaces\unskip\fontcolor{\st@textcolor}#2\shadowtext{#8}\ignorespaces\unskip%
}
%-------------------------------------------------------------------------------------------------------------------------------%
%                                             1 2  3                        4                 5            6 7   8   9
\DeclareDocumentCommand{\BreakableShadowText}{s t! D::{\tcbX@title@fontdef} D<>{0.75pt,0.1pt} D''{current} o d() d|| m}
{%\BreakableShadowText[*]'font format'<horizontal offset><vertical offset>[text color](shadow color)|double shadow color|{text}
        \unskipspacing%
        \@parse@split@csv@args{setlength}%
                <\st@shadowoffsetx,\st@shadowoffsety>%
                =<#4>%
        %\unskipspacing%
        \IfBooleanTF{#1}%
                {\shadowbasecolor{#5-coltitle}}%
                {\shadowbasecolor{#5-coltext}}%
        %\unskipspacing%
        \shadowcolor{#5-coltextshadow}%
        %\unskipspacing%
        \IfValueTF{#6}{\shadowbasecolor{#6}}{}%
        %\unskipspacing%
        \IfValueTF{#7}{\shadowcolor{#7}}{}%
        \unskipspacing%
        \IfBooleanTF{#2}%
        {%
                %\unskipspacing%
                \IfValueTF{#8}%
                {#3\doubleshadowcolor{#8}\underline{\kern-2\st@doubleshadowoffsetx\doubleshadowfy{#9}\kern2\st@doubleshadowoffsetx}}%
                {#3\uline{\shadowmathfy{#9}}}%
        }%
        {%
                %\unskipspacing%
                \IfValueTF{#8}%
                {#3\doubleshadowcolor{#8}\doubleshadowfy{#9}}%
                {#3\shadowmathfy{#9}}%
        }%
}
%-------------------------------------------------------------------------------------------------------------------------------%
%                                    1 2  3          4   5   6
\DeclareDocumentCommand{\shadowmath}{s t! O{current} d'' d() m}
{%
        \IfBooleanTF{#1}%
                {\shadowbasecolor{#3-coltitle}}%
                {\shadowbasecolor{#3-coltext}}%
        \shadowcolor{#3-coltextshadow}%
        \IfValueTF{#4}{\shadowbasecolor{#4}}{}%
        \IfBooleanTF{#5}{\shadowbasecolor{#5}}{}%
        \IfBooleanTF{#2}%
                {\mathshadow{\bmath{#6}}}%
                {\mathshadow{#6}}%
}
%-------------------------------------------------------------------------------------------------------------------------------%
%                                       1       2              3       4        5      6
\DeclareDocumentCommand{\useshadowfont}{O{navy} D<>{\doctextsize} D(){T1} D(){pag} D(){b} m}
{%
        \shadowoffset{2pt}%
        \shadowtext%
        {%
                \textcolor{#1}%
                \fontencoding{#3}%
                \fontfamily{#4}%
                \fontseries{#5}%
                \fontsize{#2}{#2}%
                \selectfont%
                #6%
        }%
}
