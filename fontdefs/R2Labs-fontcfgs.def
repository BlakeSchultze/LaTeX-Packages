\DeclareRobustCommand*{\boldstix}[2]%
{%
    \foreach \@stix@fontname/\@bold@math@version in \@stix@fontnames%
        {%
                \ifcsequal{\thecsname{#1}}{\@stix@fontname}%
                    {\xdef\math@newversion{\@bold@math@version}}{}%
        }%
    \mathversion{\math@newversion}\glb@settings%
    #1{#2}%
}
