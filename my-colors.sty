{%
        \IfStrEqCase{#1}%
        {%
                {Part}%
                {%
                        \colorlet{majorSectionHeading-colback}{LightSkyBlue}%
                        \colorlet{majorSectionHeading-colbacktitle}{MidnightBlue}%
                }%
                {Topic}%
                {%
                        \colorlet{majorSectionHeading-colback}{MidnightBlue}%
                        \colorlet{majorSectionHeading-colbacktitle}{Cerulean}%
                }%
                {Chapter}%
                {%
                        \colorlet{majorSectionHeading-colback}{LightSkyBlue}%
                        \colorlet{majorSectionHeading-colbacktitle}{MidnightBlue}%
                }%
        }%
}
