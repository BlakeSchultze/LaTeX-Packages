%\def\@tcblistings@xrightmargin{-12pt}%\dimexpr 12.33mm - 3pt\re
\def\@tcblistings@xrightmargin@sep{12pt}
\def\@tcblistings@xrightmargin%
        {\dimexpr\@tcblistings@width - \@tcblistings@linewidth - \@tcblistings@xrightmargin@sep}
\def\@tcblistings@captionwidth{\dimexpr\@tcblistings@linewidth + \@tcblistings@framexleftmargin + 9.5pt}
\def\@tcblistings@xrightmargin%
        {\dimexpr\@tcblistings@width - \@tcblistings@linewidth + \@tcblistings@xrightmargin@sep + \@tcblistings@framexrightmargin}
\def\@tcblistings@captionwidth%
        {\dimexpr\@tcblistings@linewidth - \@tcblistings@numbersep + \@tcblistings@boxrule - \@tcblistings@framexleftmargin}
\def\@tcblistings@captionleftskip%
	{\dimexpr\@tcblistings@framerule + \@tcblistings@framerule + \@tcblistings@rulesep + \@tcblistings@framexleftmargin + 1mm}
