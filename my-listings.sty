%                                                   1 2  3                           4 5
\DeclareTCBListing[blend into=listings]{tcbbashbox}{s t! D<>{\@terminal@style@linewidth} o d() }
{%
	code=
	{
		\tcbsetcurrentcolors{tcbbashboxStyle}%
		\renewcommand{\lstlistingname}{\@tcblisting@terminal@caption@title}%
		\refstepcounter{Terminallisting}
		\ifstrempty{#4}%
			{\def\@title@sep{}}%
			{\def\@title@sep{\space:\space}}%
	},
	IfBooleanTF={#1}
		{tcbbashboxStyle={\@tcb@boxrule@linewidth}{single}{stdblue}}
		{tcbbashboxStyle},
	IfBooleanTF={#2}
		{blend before title code={}}
		{blend before title code={\cftXtcbtitle@font{\@tcblisting@terminal@caption@title\ \theTerminallisting}}},
	IfBooleanTF={#2}
	{title={}}
	{
		IfValueTF={#4}
			{title={\cftXtcbtitle@font{\@title@sep#4}}}
			{title={\space}}
	},
	IfBooleanTF={#2}{top=3.0mm}{},
	width=#3,
	IfValueTF={#4}
		{list entry={\cftTerminallistingfont\protect\numberline{\thelstlisting}\@tcblisting@terminal@caption@title\ \theTerminallisting\@title@sep#4}}
		{list entry={\cftTerminallistingfont\protect\numberline{\thelstlisting}\@tcblisting@terminal@caption@title\ \theTerminallisting}},
	IfValueTF={#5}
		{comment={#5}}
		{listing only}
}
%-----------------------------------------------------------------------------------------------------------------------------------------%
\newtcbinputlisting[blend into=listings]{\Matlab}[7][]
{%\Matlab[title]{number the first line as}{first line in file}{last line in file}{caption}{label}{file}
	MatlabStyle={MatlabStyle-colframeborder}{\@tcb@boxrule@linewidth},
	code=
	{
		\let\@tcblistings@captionwidth=\@Matlab@style@captionwidth
		\let\@tcblistings@captionleftskip=\@Matlab@style@captionleftskip
		\renewcommand{\lstlistingname}{\@tcblisting@Matlab@caption@title}%
		\setcounter{@tcb@listings@counter}{\value{lstlisting}+1}
		\setcounter{lstlisting}{\value{Matlablisting}-1}
		\refstepcounter{Matlablisting}
		\ifstrempty{#1}%
			{\def\@title@sep{}}%
			{\def\@title@sep{\space:\space}}%
	},
	listing file={#7},
	blend before title code={\cftXtcbtitle@font{\@tcblisting@Matlab@caption@title\ \theMatlablisting\@title@sep}},
	list entry={\cftMatlablistingfont\protect\numberline{\the@tcb@listings@counter}\@tcblisting@Matlab@caption@title\ \theMatlablisting\@title@sep#1},
	title={\cftXtcbtitle@font{#1}},
	listing options={ style=matlabStyle, language=matlab-extended, caption={\bfmseries#5}, label={#6}, firstnumber={#2}, firstline={#3}, lastline={#4} },
	after={\setcounter{lstlisting}{\value{@tcb@listings@counter}}\par\medskip\smallskip}
}
