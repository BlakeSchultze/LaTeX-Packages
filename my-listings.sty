%                                                   1 2  3                               4   5
\DeclareTCBListing[blend into=listings]{tcbbashbox}{s t! D<>{\@terminal@style@linewidth} O{} d() }
{%
	code=
	{
		\@set@current@tcb@colors{tcbbashboxColorset}%
		\renewcommand{\lstlistingname}{\@tcblisting@terminal@caption@title}%
		\refstepcounter{Terminal}
		\ifstrempty{#4}%
			{\def\@title@sep{}}%
			{\def\@title@sep{\@tcb@title@sep}}%
	},
	IfBooleanTF={#1}
		{tcbbashboxStyle={\@tcb@default@boxrule@linewidth}{single}{tcbbashboxColorset-colfill}{tcbbashboxColorset-colinteriorborder}}
		{tcbbashboxStyle},
	listing options={style=terminalStyle, language=bash-extended, name=bash-extended},
        %every listing line*={\textcolor{lstterminalColorset-fillcolor}>\ \unskip\ignorespaces },
        %listing remove caption=false, listing and comment, listing engine=listings,
        IfBooleanTF={#2}
                {blend before title code={}}
                {blend before title code={\cftTerminaltitleblend{\@title@sep}}},
        IfBooleanTF={#2}
	{title={}}
	{
		IfValueTF={#4}
			{title={\cftXtcbtitle@font{#4}}}
			{title={\space}}
	},
	IfBooleanTF={#2}{top=3.0mm}{},
	width=#3,
	%IfValueTF={#4}
%		{list entry={\cftTerminalfont\protect\numberline{\thelstlisting}\@tcblisting@terminal@caption@title\ \theTerminal\@title@sep#4}}
%		{list entry={\cftTerminalfont\protect\numberline{\thelstlisting}\@tcblisting@terminal@caption@title\ \theTerminal}},\cftMatlablistentry{\@title@sep#1}
	IfValueTF={#4}
                {list entry={\cftTerminalfont\protect\numberline{\thelstlisting}\@tcblisting@terminal@caption@title\ \theTerminal\@title@sep#4}}
                {list entry={\cftTerminalfont\protect\numberline{\thelstlisting}\@tcblisting@terminal@caption@title\ \theTerminal}},
        IfValueTF={#5}
		{comment={#5}}
		{listing only}
}
%-----------------------------------------------------------------------------------------------------------------------------------------%
\newtcbinputlisting[blend into=listings]{\Matlab}[7][]
{%\Matlab[title]{number the first line as}{first line in file}{last line in file}{caption}{label}{file}
	MatlabStyle={6pt}{single}{MatlabColorset-colframeborder}{MatlabColorset-colinteriorborder},
	code=
	{
		\@set@current@tcb@colors{MatlabColorset}%
        	\let\@tcblistings@captionwidth=\@Matlab@style@captionwidth
		\renewcommand{\lstlistingname}{\@tcblisting@Matlab@caption@title}%
		\refstepcounter{Matlab}
		\listingcaptions
                [
                	format={tcblisting}, labelformat={Matlab}, width={\@Matlab@style@captionwidth},
                	leftskip={\@Matlab@style@captionleftskip}, aboveskip={\@Matlab@style@abovecaptionskip},
                	belowskip={\@Matlab@style@belowcaptionskip}
                ]
                \ifstrempty{#1}%
			{\def\@title@sep{}}%
			{\def\@title@sep{\@tcb@title@sep}}%
	},
	listing options=
	{
		style=matlabStyle, language=matlab-extended, title={\cftMatlabtoctitle\@tcb@title@sep#5},
		label={#6}, firstnumber={#2}, firstline={#3}, lastline={#4},
	},
        listing file={#7}, listing engine=listings, listing only, listing remove caption=false,
	blend before title code={\cftMatlabtitleblend{\@title@sep}},
        list entry={\cftMatlablistentry{\@title@sep#1}},
	title={\cftXtcbtitle@font{#1}},
	after={\par\medskip\smallskip}
}
