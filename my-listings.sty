                %\bookmark[keeplevel, dest=\bookmark@hyperaddress{Terminal}, rellevel=1]{\cftTerminal@toctitle}%
                \stepcounter{Terminal}
                \ifstrempty{#4}%
                        {\def\@title@sep{}}%
                        {\def\@title@sep{\@tcb@title@sep}}%
        },
        IfBooleanTF={#1}
                {tcbbashboxStyle={\@tcb@default@boxrule@linewidth}{single}{tcbbashboxColorset-colfill}{tcbbashboxColorset-colinteriorborder}}
                {tcbbashboxStyle},
        IfBooleanTF={#2}{top=3.0mm}{},
