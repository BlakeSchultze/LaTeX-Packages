        listing options={style=terminalStyle, language=bash-extended, name=bash-extended},
        listing remove caption=false, listing and comment, listing engine=listings,
        every listing line*={\textcolor{lstterminalColorset-fillcolor}>},
        %before upper={\vspace{-10mm}\bookmarkX@hypertarget{Terminal}},
        %before={\bookmarkX@hypertarget{Terminal}\par\bigskip},
        %after title={\bookmarkX@hypertarget{Terminal}},
        %before title={\bookmarkX@hypertarget{Terminal}},
        IfValueTF={#5}
                {comment={#5}}
                {listing only},
        IfBooleanTF={#2}
                {blend before title code={}}
                {blend before title code={\cftTerminal@hyperblend*{\@title@sep}}},
                %{blend before title code={\cftTerminal@titleblend*{\@title@sep}}},
        IfBooleanTF={#2}
        {before upper={\vspace{-7mm}\bookmarkX@hypertarget{Terminal}\vspace{-3mm}},title={}}
        {
                IfValueTF={#4}
                        %{title={\cftTerminal@tcbtitle@font*{#4}}}
                        {title={\bookmarkX@hypertarget{Terminal}\cftTerminal@hyperblend@title*{}{#4}}}
                        {title={\bookmarkX@hypertarget{Terminal}\space}}
        },
        %title={\cftfigbox@contentsline{\@title@sep#2}\cftfigbox@hypertitle{\@title@sep}{#2}}]%
        %title={\cftX@tcbtitle@font*{#2}},
        %title={\shadowmathfy{#2}},
        IfValueTF={#4}
                {list entry={\cftTerminal@listentry{\@title@sep#4}}}
                {list entry={\cftTerminal@listentry{\@title@sep}}},
        %after={\unskipspacing},
        after=
        {%
                %\par\medskip\smallskip%
                %\bookmark[keeplevel, dest=\bookmark@hyperaddress{Terminal}, rellevel=1]{\cftTerminal@toctitle}%
                \cftTerminal@bookmark%
        }
}
