% \lstname: prints the name of the current listing which is either the file name or the name defined by the name key.
% \thelstnumber \arabic{lstnumber}: prints the lines numbers.
\def\@listings@rulesep{0.75pt}							%
\def\@listings@framerule{2.0pt}							%
\def\@listings@framesep{1.0mm}
\def\@listings@numbersep{15.0pt}
\def\@listings@framexleftmargin{4.0pt}
\def\@listings@framexrightmargin{-3.0mm}
\def\@listings@framextopmargin{0.5mm}
\def\@listings@framexbottommargin{0.5mm}
\def\@listings@linewidth{17.25cm}
\def\@tcblistings@boxsep{3.0pt}
\def\@tcblistings@boxrule{2.0pt}
\def\@tcblistings@width{\dimexpr\@listings@linewidth + \@listings@numbersep + 2\@listings@rulesep + 1.5mm\relax}
\def\@tcblistings@captionwidth{\dimexpr\@listings@linewidth + \@listings@framexleftmargin + 9.5pt\relax}
\def\@tcblistings@captionleftskip{\dimexpr\@listings@framerule+\@listings@framerule+\@listings@rulesep+\@listings@framexleftmargin + 1mm\relax}
\def\@terminal@style@xleftmargin{6pt}
\def\@terminal@style@xrightmargin{-\dimexpr 9.75mm - 1.25pt\relax}
\def\@terminal@style@rulesep{0.25mm}
\def\@terminal@style@framesep{0.25mm}
\def\@terminal@style@framerule{0.25mm}
\def\@terminal@style@framexleftmargin{5pt}
\def\@terminal@style@framexrightmargin{1pt}
\def\@terminal@style@framextopmargin{3mm}
\def\@terminal@style@framexbottommargin{3mm}
\def\@terminal@style@aboveskip{0.95pt}
\def\@terminal@style@belowskip{-\dimexpr 6.25mm - 0.5pt\relax}

