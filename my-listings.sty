\lstdefinelanguage{bash-extended}
{
	%language={command.com},
	language={bash}, name=bash, float, floatplacement=b, commentstyle=\color{bash-extended-commentcolor}\textit,
	alsodigit={-},
	alsoletter={-},
	morekeywords=
    	{[1]
        	cp, rm, mv, git, mkdir, rmdir
    	},
    	keywordstyle={[1]\bfseries\color{bash-extended-keywordcolor1}},
   	morekeywords=
    	{[2]
        	pull, push, fetch, clone, status, remote, add, commit, init, diff, reset, stash, branch, checkout, merge, rm, mv, log, tag,
    	},
    	keywordstyle={[2]\bfseries\color{bash-extended-keywordcolor2}},	
    	morekeywords=
    	{[3]
        	--rebase
    	},
    	keywordstyle={[3]\bfseries\color{bash-extended-keywordcolor3}},	
}
