\DeclareCaptionFormat{stdlisting}{#1#2#3\par}
%\DeclareCaptionFormat{empty}{ }
\DeclareCaptionFormat{llap}{\llap{#1#2}#3\par}
\DeclareCaptionFormat{tcblisting}
{%
	%\begin{tcolorbox}[tcblistingCaptionStyle={\@tcblistings@captionwidth}{\@tcblistings@captionleftskip}]%
        \begin{tcolorbox}[reset, tcblistingCaptionStyle={\@tcblistings@captionwidth}]%
	%\begin{tcolorbox}[tcblistingCaptionStyle]%
        	%\unskip\ignorespaces\parbox{\textwidth}{\bfmseries#1#2#3}%
        	\unskip\ignorespaces\par\bfmseries#1#2#3
	\end{tcolorbox}\par%
}
%-----------------------------------------------------------------------------------------------------------------------------------------%
\captionsetup[lstlisting]
{%
	format=tcblisting, labelformat=\@lstlisting@caption@labelformat, labelfont={sf,white}, textfont={white,sf}, singlelinecheck=false,
	width=\@Matlab@style@width, margin=\@Matlab@style@captionleftskip, %margin=\@tcblistings@captionleftskip, %margin={1.0cm,0.0cm},
	hangindent=0cm, indention=0cm, justification=justified%, margin={1.0cm,0.0cm}, aboveskip=-2.5mm, belowskip=0.5mm
}
%-----------------------------------------------------------------------------------------------------------------------------------------%
\newkeycommand\listingcaptions
[enum format={stdlisting,tcblisting}, enum labelformat={stdlisting,Matlab,Terminal}, width={\@tcblistings@captionwidth}, leftskip={\@tcblistings@captionleftskip}, aboveskip={\@tcblistings@abovecaptionskip}, belowskip={\@tcblistings@belowcaptionskip}, counter=]
{%
	\captionsetup[lstlisting]
	{%
	        format={\commandkey{format}}, labelformat={\commandkey{labelformat}}, labelfont={sf,white}, textfont={white,sf},
                width={\commandkey{width}}, singlelinecheck=false, justification=justified, margin={\commandkey{leftskip}}, 
                aboveskip={\commandkey{aboveskip}}, belowskip={\commandkey{belowskip}} %hangindent=1cm%, indention=0cm%, 
	}
	%\let\@tcblistings@captionwidth={\commandkey{width}}
	%\tcbset{tcblistingCaptionStyle={\commandkey{width}}}
}
