\lstdefinestyle{codingStyle}
{
    	abovecaptionskip=-0.5mm,						% is the vertical space respectively above each caption.
	backgroundcolor=\color{codingStyle-backgroundcolor},	% choose the background color. You must add \RequirePackage{color}
	basewidth={0.575em,0.45em}, 					% default: {fixed,flexible mode}={0.6em,0.45em}
	basicstyle=\normalsize,               			% the size of the fonts that are used for the code
    	belowcaptionskip=0mm,						% is the vertical space respectively below each caption.	
	%boxpos=<=b|c|t>						%bottom baseline, centered or top baseline				%
	breakatwhitespace=false,                			% sets if automatic breaks should only happen at whitespace
    	breakautoindent=true,
    	%breakindent=20pt,
    	breaklines=true,                        			% sets automatic line breaking
  	% caption=\lstname,                       		% can be used instead of title
    	captionpos=b,                           			% sets the caption-position to bottom
    	columns=[c]fixed,						%=[c|l|r]<alignment > (fixed, flexible, spaceflexible, or fullflexible)
    	commentstyle=\color{codingStyle-commentcolor}\textit,    % comment style
    	%deletekeywords={...},                   			% if you want to delete keywords from the given language
    	%emptylines=[*]number						% max printed empty lines, * retains line numbering
    	extendedchars=true,                     			% lets you use non-ASCII characters; for 8-bits encodings only, does not work with UTF-8
    	fancyvrb=true,
    	fillcolor=\color{codingStyle-fillcolor},			%
    	firstnumber=auto,
    	flexiblecolumns=false,
    	float,								% tbph
    	floatplacement=t,							% is used as place specier if float is used without value
	fontadjust=false,			
    	formfeed=\bigbreak, 						% Whenever a listing contains a form feed, <tokens> is executed.
	frame=TRBL,                        				% adds a frame around the code
    	%frameround=tttt,							%
    	framerule=\@listings@framerule,					%
    	framesep=\@listings@framesep,					
    	%frameshape={RYRYNYYYY}{yny}{yny}{RYRYNYYYY},
    	framexleftmargin=\@listings@framexleftmargin,
    	framextopmargin=\@listings@framextopmargin,
    	framexbottommargin=\@listings@framexbottommargin,
    	framexrightmargin=\@listings@framexrightmargin,
    	%framextopmargin=0mm,
    	%framexbottommargin=0mm,
    	%fvcmdparams={},							%=<command1><number1>... \overlay1
    	%gobble=#								% # characters gobbled each line
    	identifierstyle=\color{codingStyle-identifiercolor},	% define style used in printing non-keyword/cs/directive text in code
    	%index=[1][keywords],
    	keepspaces=true,                        			% keeps spaces in text, useful for keeping indentation of code (possibly needs columns=flexible)
    	keywordstyle={[1]\bfseries\color{codingStyle-keywordcolor1}},% keyword style
    	keywordstyle={[2]\bfseries\color{codingStyle-keywordcolor2}},% keyword style
    	keywordstyle={[3]\bfseries\color{codingStyle-keywordcolor3}},% keyword style
    	linewidth=\@listings@linewidth,                      	% define width of code listing on page
    	%linerange={first1-last1 i,first2-last2, ...}         % The given line ranges of the (only for) individual listing are displayed.
	%lineskip=<dimension>,                      		% species additional space between lines in listings
    	%morefvcmdparams={},						%=<command1><number1>... \overlay1
    	numberblanklines=true,						% If this is set to false, blank lines get no printed line number.
    	numberfirstline=false,						% Sets numbering of first line of code:=<auto|last|number>
    	numbers=left,                           			% where to put the line-numbers
    	numbersep=\@listings@numbersep,                          			% how far the line-numbers are from the code
    	numberstyle=\tiny\bfseries\color{codingStyle-numbercolor},% the style that is used for the line-numbers
    	resetmargins=false,
	rulecolor=\color{codingStyle-rulecolor},                 % if not set, the frame-color may be changed on line-breaks within not-black text (e.g. commens (green here))
    	rulesepcolor=\color{codingStyle-rulesepcolor},		%
    	rulesep=\@listings@rulesep,					%
    	sensitive=true,							% makes keywords, control sequences, and directives case sensitive
    	showspaces=false,                       			% show spaces adding particular underscores
    	showstringspaces=false,                 			% underline spaces within strings
    	showtabs=false,                         			% show tabs within strings adding particular underscores
    	stepnumber=1,                           			% the step between two line-numbers. If it's 1, each line will be numbered
    	stringstyle=\upshape\color{codingStyle-stringcolor},     % string literal style
    	tab=$\to$,								%
    	tabsize=2,                             			% sets default tabsize to 2 spaces
    	%texcl=false,							% Activate/deactivate TeX comment lines inside code after escapeinside activated
    	%title=\lstname,                        			% show the filename of files included with \lstinputlisting; also try caption instead of title
    	upquote=true							% determines whether the left and right quote are printed  or `' (requires 'textcomp' package if true		
    	%xleftmargin=2mm,
    	%xrightmargin=-0.25cm
    	%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
    	%directives={},							% ERROR list of compiler directives
	%inputencoding=t1enc,						% ERROR determines the input encoding; requires 'inputenc' package if true
	%texcs={},								% ERROR list of control sequences
    	%usekeywordsintag=true,						% ERROR
    	%markfirstintag=false, 						% ERROR
}
