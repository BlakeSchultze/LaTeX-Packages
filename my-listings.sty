%% Usage: \cplusplus{number the first line as}{first line in file}{last line in file}{caption}{label}{file}
\newcommand{\cpluspluscode}[6]{
    \captionsetup[lstlisting]{singlelinecheck=false, width=\linewidth, margin={0.0cm,-1.0cm},justification=justified}
    \lstinputlisting[style=codingStyle, language=C++, firstnumber={#1}, firstline={#2}, lastline={#3}, caption={#4}, label={#5}]{#6}
}
%------------------------------------------------------------------------------------------------------------------------------%
%% Usage: \ccode{number the first line as}{first line in file}{last line in file}{caption}{label}{file}
\newcommand{\ccode}[6]{
    \captionsetup[lstlisting]{singlelinecheck=false, width=\linewidth, margin={0.0cm,-1.0cm},justification=justified}
    \lstinputlisting[style=codingStyle, language=C, firstnumber={#1}, firstline={#2}, lastline={#3}, caption={#4}, label={#5}]{#6}
}
%------------------------------------------------------------------------------------------------------------------------------%
%% Usage: \Verilog{number the first line as}{first line in file}{last line in file}{caption}{label}{file}
\newcommand{\Verilogcode}[6]{
    \captionsetup[lstlisting]{singlelinecheck=false, width=\linewidth, margin={0.0cm,-1.0cm},justification=justified}
    \lstinputlisting[style=codingStyle, language=Verilog, firstnumber={#1}, firstline={#2}, lastline={#3}, caption={#4}, label={#5}]{#6}
}
%------------------------------------------------------------------------------------------------------------------------------%
%% Usage: \Java{number the first line as}{first line in file}{last line in file}{caption}{label}{file}
\newcommand{\Javacode}[6]{
    \captionsetup[lstlisting]{singlelinecheck=false, width=\linewidth, margin={0.0cm,-1.0cm},justification=justified}
    \lstinputlisting[style=codingStyle, language=Java, firstnumber={#1}, firstline={#2}, lastline={#3}, caption={#4}, label={#5}]{#6}
}
