\lstdefinelanguage{diff}
{
  sensitive=true,
  % diff command line
  morecomment=[f][\fontcolor{gray}][0]{diff},
  % commit identifiers for git diff
  morecomment=[f][\fontcolor{gray}][0]{index},
  % hunk location/line numbers for unified format
  morecomment=[f][\fontcolor{blue}][0]{@@},
  % hunk location/line numbers for context format
  morecomment=[f][\fontcolor{magenta}][0]{***},
  % changed line for context format
  morecomment=[f][\fontcolor{violet}][0]{!},
  % deleted lines for unified format
  morecomment=[f][\fontcolor{red!60!black}][0]-,
  % added lines for unified format
  morecomment=[f][\fontcolor{green!60!black}][0]+,
  % file name and time stamp old file
  morecomment=[f][\fontcolor{magenta}][0]{---},
  % file name and time stamp new file
  morecomment=[f][\fontcolor{magenta}][0]{+++},
  % Binary files ... differ
  morecomment=[f][\fontcolor{gray}][0]{Binary},
  % Only in ...: file.txt
  morecomment=[f][\fontcolor{gray}][0]{Only},
  % old mode ...
  morecomment=[f][\fontcolor{gray}][0]{old},
  % new mode ...
  morecomment=[f][\fontcolor{gray}][0]{new},
  % rename from/to ...
