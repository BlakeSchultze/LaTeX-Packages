\DeclareTCBListing[blend into=listings]{tcbbashbox}{s t! D<>{\@terminal@style@linewidth} O{} d() }
{%
        code=
        {
                %\bookmarksetupnext{keeplevel}%level=\BKM@currentlevel}
                %\bookmarkX@hypertarget{Terminal}%
                %\begingroup\linespread@settings[context=Terminal]\endgroup%
        	\setlinespread{Terminal}%
