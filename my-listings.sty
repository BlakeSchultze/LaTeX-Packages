\DeclareTCBListing[blend into=listings]{tcbbashbox}{s t! o d()}
{%
	width=\linewidth,
	blend before title={colon},
	blend before title code={\hspace{-2.5mm}},
	IfBooleanTF={#1}
		{tcbbashboxStyle={stdblue}{green}}
		{tcbbashboxStyle},	
	%code={\renewcommand{\lstlistingname}{}},%\setstretch{0.5}%
	IfValueTF={#3}
	{
		code={\def\@title@text{\shadowText<\sffamily>[black](white){\space:\space#3}}},
		IfBooleanTF={#2}
			{title={\shadowText<\sffamily>[black](white){\@tcblisting@terminal@caption@title\ \theTerminallisting }\@title@text}}
			{title={\shadowText<\sffamily>[black](white){\@tcblisting@terminal@caption@title\ \theTerminallisting }\@title@text}}
	}
	{
		IfBooleanTF={#2}
			{title={}}
			{title={\shadowText<\sffamily>[black](white){\@tcblisting@terminal@caption@title\ \theTerminallisting }}}
	},
	%list text={\normalsize\bfseries\protect\numberline{\thelstlisting}\normalsize\bfseries\@tcblisting@terminal@caption@title\ \theTerminallisting },
	%list entry={\protect\numberline{\hspace{2.0em}\lstlistingname\ \thelstlisting }},
	list entry={\normalsize\bfseries\protect\numberline{\thelstlisting}\@tcblisting@terminal@caption@title\ \theTerminallisting},
	IfValueTF={#4}
		{comment={#4}}
		{listing only},
	IfValueTF={#3}{}{top=3.0mm, colback=black},
	code={\refstepcounter{Terminallisting}\par\medskip}
}
%-----------------------------------------------------------------------------------------------------------------------------------------%
\newtcbinputlisting[blend into=listings]{\Matlab}[7][]
	{
		MatlabStyle,
		code=
		{
			\renewcommand{\lstlistingname}{\@tcblisting@Matlab@caption@title}%
			\if\relax\detokenize{#1}\relax	
				\def\@title@sep{}%
		       	%blend before title code={\@tcblisting@Matlab@caption@title\ \theMatlablisting},
		    	\else
		    		\def\@title@sep{\space:\space}%%blend before title code={\@tcblisting@Matlab@caption@title\ \theMatlablisting : #1},
			\fi
		},
		listing file={#7},
		blend before title={colon},
		blend before title code={\hspace{-10mm}\sffamily\BreakableShadowText<0.65pt><0.2pt>[white](black){##1\@title@sep}},
		%list text={\normalsize\bfseries\protect\numberline{\thelstlisting}\@tcblisting@Matlab@caption@title\ \theMatlablisting },
		list entry={\normalsize\bfseries\protect\numberline{\thelstlisting}\@tcblisting@Matlab@caption@title\ \theMatlablisting},
		title={\addtocounter{lstlisting}{-1}#1},
		listing options=
		{
	   		style=codingStyle, language=matlab-extended, caption={#5}, label={#6},
			firstnumber={#2}, firstline={#3}, lastline={#4}					%			
		},
		code={\refstepcounter{Matlablisting}}
	}
