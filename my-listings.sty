\DeclareTotalTCBox{\commandbox}{s v}{verbatim,colupper=white,colback=black!75!white,colframe=black}
{%
	\IfBooleanTF{#1}{\textcolor{red}{\ttfamily\bfseries > }}{}%
	\lstinline[language=command.com, keywordstyle=\color{blue!35!white}\bfseries]^#2^%
}
%-----------------------------------------------------------------------------------------------------------------------------------------%
\DeclareTotalTCBox{\tcbinlinebashbox}{s t! v}{tcbinlinebashboxStyle}
{%
     	\IfBooleanTF{#1}{\raisebox{\dimexpr-0.25\textsize\relax}{\rule{0pt}{\textsize}}\textcolor{green}{ > }}{}%
    	\IfBooleanTF{#2}
    	{
	    	\lstinline
		[
	     		style=emptyStyle, language=bash-extended,  name=bash, identifierstyle=\textcolor{white}, commentstyle=\textcolor{white},
			stringstyle=\color{white}\upshape, showstringspaces=true, keywordstyle={[3]\color{white}},	     		
			keywordstyle={[1]\color{white}}, keywordstyle={[2]\color{white}}
	     	]^#3^%
     	}
     	{
     		\lstinline
		[
	     		language=bash-extended,  name=bash, showstringspaces=true
	     	]^#3^%
     	}
}
