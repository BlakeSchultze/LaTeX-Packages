\DeclareCaptionFont{white}{ \color{white} }
\DeclareCaptionFont{black}{ \color{black} }
\DeclareCaptionFont{blue}{ \color{blue} }
\DeclareCaptionFont{green}{ \color{green} }
%\DeclareCaptionFormat{listing}{#1#2#3\par}
%\DeclareCaptionLabelFormat{emptylabel}{\bothIfFirst{#1}{}#2}
%\DeclareCaptionFormat{empty}{ }
\DeclareCaptionFormat{listing}
{
	\begin{tcolorbox}
	[
		colback=listingsStyle-colbackcaption, colframe=listingsStyle-colframecaption, coltext=listingsStyle-coltextcaption,
		before skip=1mm, after skip=-8.5mm, left skip=-\@tcblistings@captionleftskip, right skip=0mm,
		boxsep=2pt, boxrule=2pt, arc=6pt, width=\@tcblistings@captionwidth, before=\par\smallskip,
		left=2mm, right=3mm, top=0mm, bottom=0mm, fontupper=\normalsize\bfseries
	]
	\unskip\ignorespaces\parbox{\textwidth}{#1#2#3}
	\end{tcolorbox}
}
