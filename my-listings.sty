\newtcbinputlisting[blend into=listings]{\Matlab}[7][]
{%\Matlab[title]{number the first line as}{first line in file}{last line in file}{caption}{label}{file}
        MatlabStyle={6pt}{single}{MatlabColorset-colframeborder}{MatlabColorset-colinteriorborder},
        code=
        {
                %\bookmarksetupnext{keeplevel}%level=\BKM@currentlevel}
                \@set@current@tcb@colors{MatlabColorset}%
                \let\@tcblistings@captionwidth=\@Matlab@style@captionwidth
                \renewcommand{\lstlistingname}{\@tcblisting@Matlab@caption@title}%
                %\bookmark[keeplevel, dest=\bookmark@hyperaddress{Matlab}, rellevel=1]{\cftMatlab@toctitle}%
                \stepcounter{Matlab}
                %\begingroup\linespread@settings[context=Matlab]\endgroup%
                \setlinespread{Matlab}%
                \listingcaptions
                [
                        format={tcblisting}, labelformat={Matlab}, width={\@Matlab@style@captionwidth},
                        leftskip={\@Matlab@style@captionleftskip}, aboveskip={\@Matlab@style@abovecaptionskip},
                        belowskip={\@Matlab@style@belowcaptionskip}
                ]
                \ifstrempty{#1}%
                        {\def\@title@sep{}}%
                        {\def\@title@sep{\@tcb@title@sep}}%
        },
        listing options=
        {
                style=matlabStyle, language=matlab-extended, title={\cftMatlab@tcbtitle\@tcb@title@sep#5},
                label={#6}, firstnumber={#2}, firstline={#3}, lastline={#4},
        },
        listing file={#7}, listing engine=listings, listing only, listing remove caption=false,
        %blend before title code={\cftfigboxfigure@hyperblend*'\@tcb@subtitle@font'{\@title@sep}\unskipspacing},
        %title={\cftfigboxfigure@hyperblend@title*{}{#2}},
        blend before title code={\bookmarkX@hypertarget{Matlab}\cftMatlab@hyperblend*{\@title@sep}},
        title={\cftMatlab@hyperblend@title*{}{#1}},
        %blend before title code={\cftMatlab@titleblend*{\@title@sep}},
%        title={\cftMatlab@tcbtitle@font*{#1}},
        list entry={\cftMatlab@listentry{\@title@sep#1}},
        after=
        {%
                \par\medskip\smallskip%
                \bookmark[keeplevel, dest=\bookmark@hyperaddress{Matlab}, rellevel=1]{\cftMatlab@toctitle}%
        }
}
