\DeclareDocumentCommand{\coloredtext}{s t! m m}
{%
        \begingroup%
        \IfBooleanTF{#1}
        {%
                \IfBooleanTF{#2}
                        {\ignorespaces\color{#3}\textbf{\textit{#4}}\unskip\ignorespaces}
                        {\ignorespaces\color{#3}\textbf{#4}\unskip\ignorespaces}
        }
        {%
                \IfBooleanTF{#2}
                        {\ignorespaces\color{#3}\textit{#4}\unskip\ignorespaces}
                        {\ignorespaces\color{#3}#4\unskip\ignorespaces}
        }
        \endgroup%
}
%------------------------------------------------------------------------------------------------------------------------------%
\newcommand*{\RaisedText}[1]
{%
        \begingroup
                \leavevmode
                \rlap{\kern-.4pt\raise.2pt\hbox{\color{white}#1}}%
                \rlap{\kern.4pt\raise-.2pt\hbox{\color{DarkSlateGrey!70!}#1}}%
                \hbox{#1}%
        \endgroup
}
%------------------------------------------------------------------------------------------------------------------------------%
\newcommand*{\BeveledText}[1]
{%
        \begingroup
                \leavevmode
                \rlap%
                {%
                        \kern-.3pt\raise.15pt%
                        \hbox{\pdfrender{TextRenderingMode=Stroke, LineWidth=.2pt, StrokeColor=white}#1}%
                }%
                \rlap%
                {%
                        \kern.3pt\raise-.15pt%
                        \hbox{\pdfrender{TextRenderingMode=Stroke, LineWidth=.2pt, StrokeColor=black}#1}
                }%
                \rlap{\pdfrender{TextRenderingMode=Stroke, LineWidth=.2pt, StrokeColor=DarkSlateGrey}#1}%
                \hbox{#1}%
        \endgroup
}
