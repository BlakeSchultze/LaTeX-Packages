\DeclareDocumentCommand{\BreakableShadowText}{s t! D''{current} D<>{0.75pt} D<>{0.1pt} o d() d:: m}
{%\BreakableShadowText[*]'font family/series/style/size'<horizontal offset><vertical offset>[text color](shadow color){text}
        \shadowoffsetx{#4}%
        \shadowoffsety{#5}%
        \IfBooleanTF{#1}%
                {\shadowbasecolor{#3-coltitle}}%
                {\shadowbasecolor{#3-coltext}}%
        \shadowcolor{#3-coltextshadow}%
        \IfValueTF{#6}{\shadowbasecolor{#6}}{}%
        \IfValueTF{#7}{\shadowcolor{#7}}{}%
        \IfBooleanTF{#2}%
        {%
                \IfValueTF{#8}%
                {\doubleshadowcolor{#8}\underline{\kern-2\st@doubleshadowoffsetx\doubleshadowfy{#9}\kern2\st@doubleshadowoffsetx}}%
                {\underline{\shadowmathfy{#9}}}%
        }%
        {%
                \IfValueTF{#8}%
                {\doubleshadowcolor{#8}\doubleshadowfy{#9}}%
                {\shadowmathfy{#9}}%
        }%
}
%------------------------------------------------------------------------------------------------------------------------------%
%                                    1 2                  3            4         5            6 7   8
\DeclareDocumentCommand{\shadowText}{s D::{\anttmcap16pt} D<>{0.65pt} D<>{0.2pt} D''{current} o d() m}
{%\shadowText|title font|'color style'{title text}
        \ignorespaces\unskip\setshadowTextProps[#3][#4]'#5-coltext'(#5-coltextshadow)%
        \IfBooleanTF{#1}
                {\ignorespaces\unskip\shadowbasecolor{#5-coltitle}}%
                {\ignorespaces\unskip\shadowbasecolor{#5-coltext}}%
        \IfValueTF{#6}%
                {\ignorespaces\unskip\shadowcolor{#6}}%
                {}%
        \IfValueTF{#7}%
                {\ignorespaces\unskip#2\shadowbasecolor{#7}}%
                {}%
        \ignorespaces\unskip\color{\st@textcolor}#2\shadowtext{#8}\ignorespaces\unskip%
}
%------------------------------------------------------------------------------------------------------------------------------%
\DeclareDocumentCommand{\shadowmath}{s t! O{current} d'' d() m}
{%
        \IfBooleanTF{#1}
                {\shadowbasecolor{#3-coltitle}}%
                {\shadowbasecolor{#3-coltext}}%
        \shadowcolor{#3-coltextshadow}%
        \IfValueTF{#4}{\shadowbasecolor{#4}}{}%
        \IfBooleanTF{#5}{\shadowbasecolor{#5}}{}%
        \IfBooleanTF{#2}
                {\mathshadow{\bmath{#6}}}%
                {\mathshadow{#6}}%
}
