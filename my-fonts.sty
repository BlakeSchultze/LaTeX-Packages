%-------------------------------------------------- Shadow text/math commands --------------------------------------------------%
%-------------------------------------------------------------------------------------------------------------------------------%
\long\def\shadowfypars#1\par#2\relax%
{%
        \ifstrempty{#1} {}{\shadowfywords#1 \relax\relax}%
        \ifstrempty{#2}{}{\par\shadowfypars#2\relax}%
}
%-------------------------------------------------------------------------------------------------------------------------------%
\long\def\shadowfywords#1 #2\relax%
{%
        \ifstrempty{#1}{}{\shadowfyletters#1\relax\relax}%
        \ifstrempty{#2}{}{\ \shadowfywords#2\relax}%
        \unskip%
}
%-------------------------------------------------------------------------------------------------------------------------------%
\long\def\shadowfyletters#1#2\relax%
{%
        \shadow{#1}%
        \ifx\relax#2\else\shadowfyletters#2\relax\fi\unskip%
}
%-------------------------------------------------------------------------------------------------------------------------------%
\long\def\shadowmathfypars#1\par#2\relax%
{%
        \unskipspacing\ifstrempty{#1}{\unskipspacing\noindent}{\unskipspacing\noindent\shadowmathfymathparse#1 $$\relax\noindent}%
        \ifstrempty{#2}{\unskipspacing\noindent}{\unskipspacing\noindent\shadowmathfypars#2\relax\noindent}%
}
%-------------------------------------------------------------------------------------------------------------------------------%
\long\def\shadowmathfylines#1\\#2\relax%
{%
        \ifstrempty{#1}{}{\unskipspacing\noindent\shadowmathfymathparse#1 $$\relax}%
        \ifstrempty{#2}{}{\unskipspacing\noindent\\\shadowmathfylines#2\relax\unskipspacing}%
}
%-------------------------------------------------------------------------------------------------------------------------------%
\long\def\shadowmathfymathparse#1 $#2$#3\relax%
{%
        \ifstrempty{#1}{}{\shadowfypars#1\par\relax\relax\noindent}%
        \ifstrempty{#2}{}{\noindent\space\mathshadow{#2}\space}%
        \ifstrempty{#3}{}{\noindent\shadowmathfywords#3\relax}%
}
%-------------------------------------------------------------------------------------------------------------------------------%
\long\def\shadowmathfywords#1 #2\relax%
{%
        \ifstrempty{#1}{}{\shadowmathfyletters#1\relax\relax}%
        \ifstrempty{#2}{}{\shadowmathfymathparse #2\relax}%
}
%-------------------------------------------------------------------------------------------------------------------------------%
\long\def\shadowmathfyletters#1#2\relax%
{%
        \shadow{#1}%
        \ifx\relax#2\else\shadowmathfyletters#2\relax\fi%
}
%-------------------------------------------------------------------------------------------------------------------------------%
\long\def\doubleshadowfypars#1\par#2\relax%
{%
        \ifstrempty{#1}{}{\noindent\doubleshadowfywords#1 \relax\relax}%
        \ifstrempty{#2}{}{\unskipspacing\par\noindent\doubleshadowfypars#2\relax}%
}
%-------------------------------------------------------------------------------------------------------------------------------%
\long\def\doubleshadowfywords#1 #2\relax%
{%
        \ifstrempty{#1}{}{\doubleshadowfyletters#1\relax\relax}%
        \ifstrempty{#2}{}{\ \doubleshadowfywords#2\relax}%
        \unskip\ignorespaces%
}
%-------------------------------------------------------------------------------------------------------------------------------%
\long\def\doubleshadowfyletters#1#2\relax%
{%
        \kern\st@shadowoffsetx\doubleshadow{#1}%
        \ifx\relax#2\else\doubleshadowfyletters#2\relax\fi\unskip%
}
%-------------------------------------------------------------------------------------------------------------------------------%
\long\def\shadow#1%
{%
        \kern\st@shadowoffsetx%
        \raisebox{\st@shadowoffsety}%
        {%
                \stackinset{c}{-\st@shadowoffsetx}{c}{-\st@shadowoffsety}%
                        {\fontcolor{\st@textcolor}{#1}}%
                        {\fontcolor{\st@shadowcolor}{#1}}%
        }%
        \kern-\st@shadowoffsetx%
}
%-------------------------------------------------------------------------------------------------------------------------------%
\long\def\mathshadow#1{\shadow{\ensuremath{#1}}}
%-------------------------------------------------------------------------------------------------------------------------------%
\long\def\doubleshadow#1%
{%
        \kern\st@doubleshadowoffsetx%
        \raisebox{\st@doubleshadowoffsety}%
        {%
                \stackinset{c}{\st@doubleshadowoffsetx}{c}{-\st@doubleshadowoffsety}%
                        {\shadow{#1}}%
                        {\fontcolor{\st@doubleshadowcolor}{#1}}%
        }%
        \kern-\st@doubleshadowoffsetx%
}
%-------------------------------------------------------------------------------------------------------------------------------%
\newcommand\shadowfy[1]{\xp\shadowfypars#1\par\relax\relax}
\newcommand\shadowmathfy[1]{\unskipspacing\xp\shadowmathfypars#1\par\relax\relax\relax\noindent}
\newcommand\doubleshadowfy[1]{\xp\doubleshadowfypars#1\par\relax\relax}
