%-------------------------------------------------------------------------------------------------------------------------------%
\newcommand{\ds}[1]{%
        \toggletrue{@macro@toggle}
        \edef\@arg@input{#1}
        \edef\@string@input{\@nil\detok@csname{\@arg@input}}
        \fullyexpand{#1}\\
        \detokenize{#1}\\
        \xp\detokenize\xp{\fullyexpand{#1}}\\
        \secondoftwo#1
        \@arg@input\\\@string@input\\
        \def\@string@result{}
        \@tfor\next:=\@arg@input\do%
        {%
                =\next\\
                \IfInteger{\next}{\togglefalse{@macro@toggle}}{}%
                \iftoggle{@macro@toggle}{\appto{\@string@result}{\next}}{}%
                %\StrBefore{xstring}{tri}
                %\dX{\next}%
        }%
        \@string@result\\
}
