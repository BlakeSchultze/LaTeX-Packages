\DeclareDocumentCommand{\polyDivision}{s t` o D(){\rparen} m m d() o s}
{
	\IfBooleanTF{#1}%
		{\toggletrue{@pld@print@zero@coefficients}}%	
		{\togglefalse{@pld@print@zero@coefficients}}%
	\let\@pld@open@delimiter=#4%
	\addtocounter{@pld@instances}{1}
	\IfValueTF{#7}{\polyset{#7}}{}%
	\polylongdiv{#5}{#6}
     	\ifx\pld@remainder\@empty%
		\setlength{\@pld@tcb@width}{\maxof{\@pld@solution@box@width}{\@pld@conclusion@box@width}}\unskip\ignorespaces%	
		\addtolength{\@pld@tcb@width}{-0.5mm}%
	\else%
		\setlength{\@pld@tcb@width}{\maxof{\@pld@solution@box@width}{\@pld@conclusion@box@width}}\unskip\ignorespaces%	
		\addtolength{\@pld@tcb@width}{13mm}%
	\fi%
	\renewcommand{\baselinestretch}{1.2}
	\IfBooleanTF{#2}%
	{
		\IfValueTF{#3}
			{\def\@pld@title{\@tcb@pld@title@label{$f(x)=\dfrac{#5}{#6}$}\\[0.2em]\flushleft\large\normalfont#3}}
			{\def\@pld@title{\@tcb@pld@title@label{$f(x)=\dfrac{#5}{#6}$}\\[0.2em]\flushleft\normalsize\normalfont}}
	}
	{\IfValueTF{#3}{\def\@pld@title{{#3}}}{\def\@pld@title{{}}}}%
	\IfBooleanTF{#2}
		{\tcbset{pldTitledStyle, title={\@pld@title}, toptitle = 1mm, width=\@pld@tcb@width}}
		{
			\IfValueTF{#3}
				{\tcbset{pldTitledStyle, title={\@pld@title}, width=\@pld@tcb@width}}
				{\tcbset{pldStyle, width=\@pld@tcb@width, toptitle=1mm}}
		}
	%\tcbset{title={\@pld@title}}
	\IfValueTF{#8}{\tcbset{#8}}{}%
     	\IfValueTF{#3}
		{\phantomsection\addcontentsline{lot}{subsection}{\large Polynomial Division  \arabic{@pld@instances}: #3}}
		{\phantomsection\addcontentsline{lot}{subsection}{\large Polynomial Division \arabic{@pld@instances}}}
	\IfBooleanTF{#9}
	{
		\begin{tcolorbox}%--
	        	\usebox{\@pld@computation@box}%
			\ifx\pld@remainder\@empty%
			\else%
				\usebox{\@pld@remainder@box}%
			\fi%
			\\%
			\usebox{\@pld@conclusion@box}%%
		\end{tcolorbox}
	}
	{
		\begin{tcolorbox}%--
	        	\usebox{\@pld@computation@box}%
			\ifx\pld@remainder\@empty%
			\else%
				\usebox{\@pld@remainder@box}%
			\fi%
		\end{tcolorbox}
	}
	\renewcommand{\baselinestretch}{1.2}
	\@reset@ul@settings
	\tcbset{reset}
}
