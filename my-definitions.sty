\def\endSbox
{%
        \@testtrue
        \ifhmode
                \ifinner\@testfalse\fi
        \fi
        \if@test\@latexerr
                {Misplaced \string\endSbox! Should be in LR mode}\@ehd
        \fi
        \egroup
}
