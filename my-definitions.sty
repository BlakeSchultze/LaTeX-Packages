\newcommand{\repfrac}{.33}									% Sets the dot/dash/etc. repetition length as a fraction of defined \replength
\newcommand{\dashfrac}[1]{\renewcommand\repfrac{#1}}					% Sets the dash length as a fraction of \replength
\newcommand{\rulewidth}{.6pt}									% Sets line width of horizontal rule in t(ab)dotfill and t(ab)dashfill commands
\newcommand{\csvdel}{}										% Removes following token from input stream
\newcommand{\gobblenext}[1]{\csvdel#1\@ifnextchar\bgroup{\gobblenext}{}}	% Gobble next "argument"
\newcommand{\checknextarg}{\@ifnextchar\bgroup{\gobblenext}{}}			% Check if another "argument" exists
\newcommand{\cshadowbox}{\VerbBox\@cshadowbox}						% Create shadowed box (from fancybox package: fancybox.sty)
\newcommand{\probtag}[1]{\tag*{\bfseries(#1)}\addtocounter{equation}{-1}}	% Adds tag text (#1) to equation line
\newcommand{\deftag}[1]{\tag*{\bfseries(#1)}\addtocounter{equation}{-1}}	% Adds tag text (#1) to equation line
\newcommand{\soln}{\textit{(Sol.) \solnshift}}					% Provides classic "(Sol.)" text label used to mark the beginning of a solution
\WithSuffix\newcommand{\soln}*{\textbfit{(Sol.) }\solnshift}		% Provides starred version of \soln command to make "(Sol.)" text label bold
\newcommand{\solnshift}{\hspace{-0.4cm}}						% Shift after (Sol.) text label to properly position beginning of solution text
