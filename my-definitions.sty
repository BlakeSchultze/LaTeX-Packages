%---------------------------------------------- Counter Increment/Print Commands ----------------------------------------------%
%-------------------------------------------------------------------------------------------------------------------------------%
\def\stepcount#1{\advance#1 1\relax}                                    % Increase count by 1
\def\addtocount#1#2{\advance#1 #2\relax}                                % Add #2 to count
\def\setcount#1#2{#1 #2\relax}                                          % Set count to #2
\def\incrementcount#1{\stepcount{#1}}                                   % Increase count by 1
\def\decrementcount#1{\addtocount{#1}{-1}}                              % Decrease count by 1
\def\thecount#1{\the#1}                                                 % Generate string from count value
\def\printcount#1{\number#1}                                            % Generate string from count value
%-------------------------------------------------------------------------------------------------------------------------------%
\def\incrementcounter#1{\stepcounter{#1}}                               % Increase counter by 1
\def\decrementcounter#1{\addtocounter{#1}{-1}}                          % Decrease counter by 1
\def\tempcounter#1{\setcounter{@macro@counter}{#1}}                     % Generate string from counter value
\def\@print@before@increment#1#2{#1{#2}\stepcounter{#2}}                % Print/increment counter #2 w/ format #1 (e.g. \alph)
\def\thecounter#1{\the\value{#1}}                                       % Generate string from counter value
\def\printcounter#1#2{#1{#2}\relax}                                     % Generate string from counter value
%-------------------------------------------------------------------------------------------------------------------------------%
\def\steplinkedcounter#1#2%
{%
        \stepcounter{#2}%
        \setcounter{#1}{\the\value{#2}}%
}
%-------------------------------------------------------------------------------------------------------------------------------%
\def\refsteplinkedcounter#1#2%
{%
        \refstepcounter{#2}%
        \setcounter{#1}{\the\value{#2}}%
}
%-------------------------------------------------------------------------------------------------------------------------------%
\def\stepsectioncounter#1%
{%
        \IfStrEqCase{#1}
        {
                {Part}{\steplinkedcounter{Part}{part}}%
                {part}{\steplinkedcounter{part}{Part}}%
                {Topic}{\stepcounter{Topic}}%
                {Chapter}{\steplinkedcounter{Chapter}{chapter}}%
                {Section}{\steplinkedcounter{Section}{section}}%
                {Subsection}{\steplinkedcounter{Subsection}{subsection}}%
        }%
}
%-------------------------------------------------------------------------------------------------------------------------------%
\def\refstepsectioncounter#1%
{%
        \IfStrEqCase{#1}
        {
                {Part}{\steplinkedcounter{Part}{part}}%
                {part}{\steplinkedcounter{part}{Part}}%
                {Topic}{\stepcounter{Topic}}%
                {Chapter}{\steplinkedcounter{Chapter}{chapter}}%
                {Section}{\steplinkedcounter{Section}{section}}%
                {Subsection}{\steplinkedcounter{Subsection}{subsection}}%
        }%
}
