% Calculate length of alignat environment
\DeclareDocumentCommand{\settowidthofalign}{m m}
{%
    \setbox\z@=\vbox
    {
        \begin{alignat*}{2}
            #2
            \ifmeasuring@\else\global\let\got@maxcolwd\maxcolumn@widths\fi
        \end{alignat*}
    }%
    \begingroup
    \def\or{+}\edef\x{\endgroup#1=\dimexpr\got@maxcolwd\relax}\x
}
