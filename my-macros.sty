   	\stepcounter{@count@problem}
   	\IfBooleanTF{#2}
    	{
        	\tcbset{problemsStyle}
        	\setcounter{equation}{1}
        	\setcounter{@subprob@counter}{1}
    	}
    	{\tcbset{subproblemsStyle}}
    	\IfBooleanTF{#2}%
    	{%
    		\IfValueTF{#4}%
        	{%
        		\IfBooleanTF{#5}%
			{%
				\problemparts{(\roman{@prob@part@counter})}%
		    	}%
			{%				
        			\setcounter{@prob@part@counter}{1}
				\setcounter{@chap@counter}{#3}%
            		\setcounter{@prob@counter}{#4}%
            		\problems{\arabic{@chap@counter}.\arabic{@prob@counter}}%
            	}%
		}{\problems{\arabic{@prob@counter}}}%    		
		\@end@macro@spacing%
    	}%
    	{
    		\IfBooleanTF{#5}%
		{
			\IfValueTF{#4}%
			{\problempartssubproblems{(\alph{@subprob@counter})}}
	        	{\subproblemparts{(\roman{@prob@part@counter})}}
		}
	    	{\subproblems{(\alph{@subprob@counter})}}
    	}    				  	    	
    	\IfValueTF{#1}
    		{\tcbset{#1}}{}
