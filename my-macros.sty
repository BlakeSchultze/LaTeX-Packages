% Insert a blank page, with (default) or without (starred version) increasing the page #
\DeclareDocumentCommand{\blankpage}{s}
{
    \newpage%
    \thispagestyle{empty}%
    \mbox{}%
    \newpage%
    \IfBooleanTF{#1}{\decrementcounter{page}}{}
}
%-------------------------------------------------------------------------------------------------------------------------------%
\DeclareDocumentCommand{\@section@sep}{m m}
{       %
        \begingroup%
        \coverpagelinespacing%
        \@enforce@sizeX%
        \centering\minibox%
        {
                \fontcolor{#1}\rule{\textwidth}{1pt}\\[\dimexpr-10pt-3.5pt\relax]
                \fontcolor{#2}\rule{\textwidth}{1pt}\\[\dimexpr-10pt-3.5pt\relax]
                \fontcolor{#2}\rule{\textwidth}{1pt}\\[\dimexpr-1em-3.5pt\relax]
                \fontcolor{#1}\rule{\textwidth}{1pt}\\[\dimexpr-10pt-3.5pt\relax]%
                %\par\smallskip%
        }%
        \endgroup%
}
%-------------------------------------------------------------------------------------------------------------------------------%
\DeclareDocumentCommand{\baylorsection}{}{\@section@sep{BaylorGreenCMYK}{BaylorGoldCMYK}}
%-------------------------------------------------------------------------------------------------------------------------------%
%                                       1 2  3                        4 5                  6
\DeclareDocumentCommand{\sectionheader}{s t! O{\tcbsectionheaderfont} m O{BaylorGreenCMYK} O{BaylorGoldCMYK}}
{%
        %\begingroup%
        \setlength{\baselineskip}{.35in}\let\parindent\@par@indent%
        \setlength{\parindent}{0pt}%
        \settowidth\@macro@width{\shadowtext{#3{#4}}}%
        \shadowcolor{#6}%
        \color{#5}%
        \par\vspace{\parskip}%
        \parbox{\textwidth}%
        {%
                \@section@sep{#5}{#6}\\[-.75em]%
                \par%
                \begingroup%
                \@enforce@sizeX
                \centering%
                \pgfifthenelse(\@macro@width >= \linewidth)%
                        {\medskip\shadowtext{\parbox{\textwidth}{\raggedright#3{#4}}}\smallskip}%
                        {\shadowtext{\raggedright#3{#4}}\\[-1em]}%
                \endgroup%
                \coverpagelinespacing%
                \@section@sep{#5}{#6}%
                \par\medskip%
        }%
        %\endgroup%
        \let\parindent\@par@indent%
}
%-------------------------------------------------------------------------------------------------------------------------------%
%                                                     1 2  3  4  5
\DeclareDocumentCommand{\tcbequation@parse@booleans}{s t+ t!}
{%
        \IfBooleanTF{#1}%
                {\@set@current@tcb@colors{tcbequationColorset}}%
                {\@set@current@tcb@colors{tcbsolutionColorset}}%
        \IfBooleanTF{#2}{\gdef\@tcbequation@finish@skip{[0.85em]}}{\gdef\@tcbequation@finish@skip{}}%
        \IfBooleanTF{#3}{\gdef\@tcbequation@finish{\xp\notag\xp\\\@tcbequation@finish@skip}}{\xp\gdef\xp\@tcbequation@finish\xp{\xp\\\@tcbequation@finish@skip}}%
}
%-------------------------------------------------------------------------------------------------------------------------------%
%                                                          1 2  3  4
\DeclareDocumentCommand{\tcbequation@parse@arrow@booleans}{m t- t> t^}
{%
        \IfBooleanTF{#2}%
                {\setcounter{@tcb@target@equationbox@counter}{\value{@tcb@equationbox@counter} - #1}}%
                {\setcounter{@tcb@target@equationbox@counter}{#1}}%
        \IfBooleanTF{#3}%
        {%
                \gdef\@source@anchor@position{east}%
                \gdef\@target@anchor@position{east}%
                \gdef\@bend@value{-45}%
        }%
        {%
                \gdef\@source@anchor@position{west}%
                \gdef\@target@anchor@position{west}%
                \gdef\@bend@value{45}%
        }%
        \IfBooleanTF{#4}%
        {%
                \xdef\@tcbequation@source@node{frame}%
                \xdef\@tcbequation@target@node{\@tcbequation@box@prefix\roman{@tcb@target@equationbox@counter}}%
                \xdef\@bend@value{-\@bend@value}%
        }%
        {%
                \xdef\@tcbequation@source@node{\@tcbequation@box@prefix\roman{@tcb@target@equationbox@counter}}%
                \xdef\@tcbequation@target@node{frame}%
        }%
        % Validate target \tcbequation exists
        \ifnumgreater{\value{@tcb@target@equationbox@counter}}{\value{@tcb@equationbox@counter}}%
                {\errmessage{ERROR: \errcmd\tcbequation \hash\arabic{@tcb@target@equationbox@counter} does not exist at this point in the document}}{}%
        \ifnumequal{\value{@tcb@target@equationbox@counter}}{\value{@tcb@equationbox@counter}}%
                {\errmessage{ERROR: A \errcmd{\tcbequation's} reference arrows cannot point to itself}}{}%
}
%-------------------------------------------------------------------------------------------------------------------------------%
% Boxed equation maintaining alignment w/ previous equations using '&' alignment character.  Each
%                                     1     2 3 4 5     6   7  8                 9
\DeclareDocumentCommand{\tcbequation}{D(){} m m g D(){} d<> t| D<>{-0.1pt,0.1pt} O{equationboxStyle}}
{%\tcbequation(*>=-){<LHS>}{<RHS>}[</tcb/.style>]|alt. remember name|(explicit ref name){ref num}<src anchor, target anchor>
        &%
        \begingroup%
                \settowidth\@macro@width{$\displaystyle\boldsymbol{#2}$}%
                \addtolength\@macro@width{\dimexpr 2mm+4pt\relax}%
                %\addtolength\@macro@width{4\fboxsep+4\fboxrule}%
                %\addtolength\@macro@width{2mm+4pt}%
                \hspace{-\@macro@width}%
        \endgroup%
        \tcbequation@parse@booleans#1%
        \stepcounter{@tcb@equationbox@counter}%
        \xdef\@tcbequation@box@name{\@tcbequation@box@prefix\roman{@tcb@equationbox@counter}}%
        \IfValueTF{#4}%
        {%
                \tcbequation@parse@arrow@booleans{#4}#5%
                \IfValueTF{#6}{\@parse@split@csv@args{gdef}<@source@anchor@position,@target@anchor@position,@bend@value>=<#6>}{}%
                \xdef\@tcbequation@source{\@tcbequation@source@node.\@source@anchor@position}%
                \xdef\@tcbequation@target{\@tcbequation@target@node.\@target@anchor@position}%
                \@parse@split@csv@args{gdef}<@source@anchor@yshift,@target@anchor@yshift>=<#8>%\
                \tcbhighmath%
                [%
                        #9, remember as=\@tcbequation@box@name,
                        overlay=%
                        {%
                                \draw[tcbequationArrows]
                                        ([yshift=\@source@anchor@yshift]\@tcbequation@source)
                                        to[bend right=\@bend@value]
                                        ([yshift=\@target@anchor@yshift]\@tcbequation@target);
                        }%
                ]%
                {\boldsymbol{#2 #3}}%
        }%
        {\tcbhighmath[#9,remember as=\@tcbequation@box@name]{\boldsymbol{#2 #3}}}%
        \@tcbequation@finish%
}
