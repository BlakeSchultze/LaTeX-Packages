    	\begin{tcolorbox}
	\IfBooleanTF {#2}
    	{
        	\IfValueTF{#4}
        	{
        		\IfBooleanTF{#5}
			{
		        	\currentpdfbookmark
					{(\arabic{@chap@counter}.\arabic{@prob@counter})\roman{@prob@part@counter}}
					{problem\arabic{@chap@counter}.\arabic{@prob@counter}\roman{@prob@part@counter}}				
		        	(\roman{@prob@part@counter})
				\stepcounter{@prob@part@counter}
		        	\ignorespaces
		    	}
			{
            		\currentpdfbookmark
            			{(\arabic{@chap@counter}.\arabic{@prob@counter})}
            			{problem\arabic{@chap@counter}.\arabic{@prob@counter}}
            		(\arabic{@chap@counter}.\arabic{@prob@counter})
            		\ignorespaces
            	}
		}{(\arabic{@prob@counter}) \ignorespaces}
	}
    	{
    		\IfBooleanTF{#5}%
		{
			\currentpdfbookmark
        		{(\roman{@prob@part@counter})}
        		{problem\alph{@prob@part@counter}}
        	(\roman{@prob@part@counter})
        	%\addtocounter{@prob@part@counter}{1}\ignorespaces
		}
	    	{
			\currentpdfbookmark
        			{(\alph{@subprob@counter})}
        			{problem\alph{@subprob@counter}}
        			(\alph{@subprob@counter})
        			\addtocounter{@subprob@counter}{1}\ignorespaces
        	}       	
    	}
}{\end{tcolorbox}\egroup\setlength{\parindent}{\@macro@length}}
