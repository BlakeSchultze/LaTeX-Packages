{%tcbfunctionlistbox[*]<box width>'color scheme'{style}[title][tcbset options](tcbseteverylayer options)
	\setlength{\@function@listbox@first@column}{#8}
    	\setlength{\@function@listbox@second@column}{#9}
    	\let\@current@listbox@first@column=\@function@listbox@first@column
	\let\@current@listbox@second@column=\@function@listbox@second@column
	\let\@current@listbox@column@sep=\@function@listbox@column@sep
	\addtocounter{table}{-1}
    	\tcbsetcurrentcolors{#3}
	\tcbsetforeverylayer{#4, width=#2, breakable}
   	\IfValueTF{#6}{\tcbset{#6}}{}
    	\IfValueTF{#7}{\tcbsetforeverylayer{#7}}{}
    	\tcolorbox[#4, title={#5}, breakable]
    	\IfBooleanTF{#1}
  		{\hspace{0pt}\begin{longtable}{H{\@function@listbox@first@column} H{\@function@listbox@second@column}}}
   		{\hspace{0pt}\begin{longtable}{p{\@function@listbox@first@column} p{\@function@listbox@second@column}}}
}{\end{longtable}\endtcolorbox}
