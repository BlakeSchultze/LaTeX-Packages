%------------------------------------------------------------------------------------------------------------------------------%
\DeclareDocumentCommand{\@init@if}{m O{false}}
{%
        \ifismacro{#1}%
        {%
                \newif#1%
                \xdef\@the@if@name{\theifname{#1}}%
                \expandafter\csuse\expandafter{\theifname{#1}#2}%
                \ifcsundef{the\@the@if@name}{\csgdef{the\@the@if@name}{\@print@if@value{\thecsname{#1}}}}{}%
        }%
        {%
                \expandafter\newif\csname if#1\endcsname\csuse{#1#2}%
                \csuse{if#1#2}%
                \ifcsundef{the#1}{\csgdef{the#1}{\@print@if@value{if#1}}}{}%
        }%
}
%------------------------------------------------------------------------------------------------------------------------------%
\DeclareDocumentCommand{\@init@counter}{m O{0}}{\newcounter{#1}\setcounter{#1}{#2}}
%------------------------------------------------------------------------------------------------------------------------------%
\DeclareDocumentCommand{\@init@toggle}{s m}%
{%
	\newtoggle{#2}%
	\IfBooleanTF{#1}{\toggletrue{#2}}{\togglefalse{#2}}%
	\ifcsundef{the#2}{\csgdef{the#2}{\@print@toggle@value{#2}}}{}%
}
%------------------------------------------------------------------------------------------------------------------------------%
\DeclareDocumentCommand{\@init@skip}{m m O{0pt} O{0pt}}
{%
        \ifismacro{#1}%
        {%
                \newskip#1%
                #1=#2 plus #3 minus #4%
                \ifcsundef{the\thecsname{#1}}{\csgdef{the\thecsname{#1}}{\the#1}}{}%
        }%
        {%
                \expandafter\newskip\csname #1\endcsname%
                \csname#1\endcsname=#2 plus #3 minus #4%
                \ifcsundef{the#1}{\csgdef{the#1}{\expandafter\the\csname#1\endcsname}}{}%
        }%
}
%------------------------------------------------------------------------------------------------------------------------------%
\DeclareDocumentCommand{\@init@dimen}{m O{0pt}}
{%
        \ifismacro{#1}%
        {%
                \newdimen#1%
                \setlength#1{#2}
                \ifcsundef{the\thecsname{#1}}{\csgdef{the\thecsname{#1}}{\the#1}}{}%
        }%
        {%
                \expandafter\newdimen\csname #1\endcsname\expandafter%
                \setlength\csname #1\endcsname{#2}%
                \ifcsundef{the#1}{\csgdef{the#1}{\expandafter\the\csname#1\endcsname}}{}%
        }%
}
%------------------------------------------------------------------------------------------------------------------------------%
\DeclareDocumentCommand{\@init@length}{m O{0pt}}%
{%
	\ifismacro{#1}%
        {%
                \newlength#1%
                \setlength#1{#2}
                \ifcsundef{the\thecsname{#1}}{\csgdef{the\thecsname{#1}}{\the#1}}{}%
        }%
        {%
        	\expandafter\newlength\csname #1\endcsname\expandafter%
	        \setlength\csname #1\endcsname{#2}%
	        \ifcsundef{the#1}{\csgdef{the#1}{\expandafter\the\csname#1\endcsname}}{}%
        }%
}
%------------------------------------------------------------------------------------------------------------------------------%
\DeclareDocumentCommand{\@counter@without}{s t! m o}%
{%\@cftX@namenums@parser{list entry}{name}[long name]{numberline prefix}(alt. numberline counter)/alt. title counter/
        \IfValueTF{#4}%
        {\def\@detaching@from{#4}}%
        {%
                \IfBooleanTF{#1}%
                    {\def\@detaching@from{section}}%
                    {\def\@detaching@from{chapter}}%
        }%
        \IfBooleanTF{#2}%
                {\AtBeginDocument{\expandafter\counterwithout\expandafter{#3}{\@detaching@from}}}%
                {\AfterPreamble{\expandafter\counterwithout\expandafter{#3}{\@detaching@from}}}%%
}
%------------------------------------------------------------------------------------------------------------------------------%
\DeclareDocumentCommand{\@counterwithinout@counter@list}{s m}
{
        \IfBooleanTF{#1}
        {
                \@for\@current@counter:=#2\do%
                {\expandafter\ifdefined\csname\@current@counter\endcsname\expandafter\counterwithout\expandafter{\@current@counter}{chapter}\fi}
        }
        {       \@for\@current@counter:=#2\do%
                {\expandafter\ifdefined\csname\@current@counter\endcsname\expandafter\counterwithin\expandafter{\@current@counter}{chapter}\fi}
        }
}
%------------------------------------------------------------------------------------------------------------------------------%
\DeclareDocumentCommand{\@setlength@operations@by@name}{m m m}
{%
        \IfStrEqCase{#1}%
        {%
                {quarter}{\setlength{\@csname{#2}}{\dimexpr#3 / 4\relax}}%
                {half}{\setlength{\@csname{#2}}{\dimexpr#3 / 2\relax}}%
                {threequarter}{\setlength{\@csname{#2}}{\dimexpr#3 * 3/4\relax}}%
                {oneandhalf}{\setlength{\@csname{#2}}{\dimexpr#3 * 3/2\relax}}%
                {oneandthreequarter}{\setlength{\@csname{#2}}{\dimexpr#3 * 7/4\relax}}%
                {twice}{\setlength{\@csname{#2}}{\dimexpr#3 * 2\relax}}%
        }[Unknown length operation name #1 given]%
}
%------------------------------------------------------------------------------------------------------------------------------%
\DeclareDocumentCommand{\@tcb@init@X@linewidths}{s m o g}
{%
        \@init@length{@tcb@#2@linewidth}%
        \IfBooleanTF{#1}%
        {%
                \@init@length{@tcb@#2@xshift}%
                \@init@length{@tcb@#2@yshift}%
        }{}%
        \IfValueTF{#3}{\@init@length{@tcb@default@#2@linewidth}[#3]}{}%
        \IfValueTF{#4}%
                {\ForEachX{,}{\@init@length{@tcb@\thislevelitem @#2@linewidth}}{#4}}%
                {\ForEachX{,}{\@init@length{@tcb@\thislevelitem @#2@linewidth}}{\@setlength@operation@names}}%
        \expandafter\newcommand\csname @tcb@set@#2@linewidths\endcsname[1]%
        {%
                \expandafter\setlength\csname @tcb@#2@linewidth\endcsname{##1}%
                \@ifundefined{@tcb@quarter@#2@linewidth}{}%
                        {\@setlength@operations@by@name{quarter}{@tcb@quarter@#2@linewidth}{##1}}%
                \@ifundefined{@tcb@half@#2@linewidth}{}%
                        {\@setlength@operations@by@name{half}{@tcb@half@#2@linewidth}{##1}}%
                \@ifundefined{@tcb@threequarter@#2@linewidth}{}%
                        {\@setlength@operations@by@name{threequarter}{@tcb@threequarter@#2@linewidth}{##1}}%
                \@ifundefined{@tcb@oneandhalf@#2@linewidth}{}%
                        {\@setlength@operations@by@name{oneandhalf}{@tcb@oneandhalf@#2@linewidth}{##1}}%
                \@ifundefined{@tcb@oneandthreequarter@#2@linewidth}{}%
                        {\@setlength@operations@by@name{oneandthreequarter}{@tcb@oneandthreequarter@#2@linewidth}{##1}}%
                \@ifundefined{@tcb@twice@#2@linewidth}{}%
                        {\@setlength@operations@by@name{twice}{@tcb@twice@#2@linewidth}{##1}}%
        }%
        \IfValueTF{#3}{\csname @tcb@set@#2@linewidths\endcsname{#3}}{}%
}
