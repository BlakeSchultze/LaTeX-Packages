\DeclareDocumentCommand{\@set@tcb@env@border@colors}{m m}
{
        %\@set@specific@tcb@color{colframe}{#1}
        \@set@specific@tcb@color{colframeborder}{#1}
        \@set@specific@tcb@color{coldoubleline}{#2}
